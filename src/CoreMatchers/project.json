{
  "authors": [ "Jim Popadak", "Bryan Herman" ],
  "configurations": {
    "Debug": {
      "buildOptions": {
        "debugType": "full",
        "define": [ "DEBUG", "TRACE" ],
        "warningsAsErrors": true
      }
    },
    "Release": {
      "buildOptions": {
        "debugType": "none",
        "define": [ "RELEASE", "TRACE" ],
        "warningsAsErrors": true,
        "optimize": true
      }
    }
  },
  "dependencies": {
    "NETStandard.Library": "1.6.1-preview1-24530-04",
    "System.Reflection.TypeExtensions": "4.3.0-preview1-24530-04"
  },
  "description": "An implementation of assertion matchers for testing.",
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },
  "packOptions": {
    "licenseUrl": "https://api.github.com/licenses/bsd-3-clause",
    "owners": [ "Jim Popadak" ],
    "releaseNotes": "Finished all xUnit tests",
    "repository": {
      "type": "git",
      "url": "https://github.com/jpopadak/CoreMatchers"
    },
    "summary": "An implementation of assertion matchers for testing.",
    "requireLicenseAcceptance": false,
    "tags": [ ".NET", "DNX", "Core", "Matchers", "Test", "Hamcrest" ]
  },
  "version": "1.0.0-*"
